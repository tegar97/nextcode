{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { useEffect, useState } from \"react\";\nimport Head from \"next/head\";\nimport { useTheme } from \"next-themes\";\nexport default function Home() {\n  const {\n    0: isMounted,\n    1: setIsMounted\n  } = useState(false);\n  const {\n    theme,\n    setTheme\n  } = useTheme();\n  useEffect(() => {\n    setIsMounted(true);\n  }, []);\n\n  const switchTheme = () => {\n    if (isMounted) {\n      setTheme(theme === \"light\" ? \"dark\" : \"light\");\n    }\n  };\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: \"text-center\",\n    children: [/*#__PURE__*/_jsxs(Head, {\n      children: [/*#__PURE__*/_jsxs(\"title\", {\n        children: [\"Dark mode with Tailwind and \", theme]\n      }), /*#__PURE__*/_jsx(\"link\", {\n        rel: \"icon\",\n        href: \"/favicon.ico\"\n      })]\n    }), /*#__PURE__*/_jsx(\"h1\", {\n      className: \"text:2xl\",\n      children: \"Dark mode with Tailwind and Next- themes\"\n    }), /*#__PURE__*/_jsx(\"button\", {\n      onClick: switchTheme,\n      children: \"Change theme\"\n    })]\n  });\n}","map":null,"metadata":{},"sourceType":"module"}