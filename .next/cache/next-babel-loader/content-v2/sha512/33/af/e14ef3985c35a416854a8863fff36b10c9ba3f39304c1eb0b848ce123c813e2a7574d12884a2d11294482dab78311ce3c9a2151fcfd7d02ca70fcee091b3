{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useEffect, useState } from \"react\";\nimport Head from \"next/head\";\nimport { useTheme } from \"next-themes\";\nexport default function Home() {\n  var _useState = useState(false),\n      isMounted = _useState[0],\n      setIsMounted = _useState[1];\n\n  var _useTheme = useTheme(),\n      theme = _useTheme.theme,\n      setTheme = _useTheme.setTheme;\n\n  useEffect(function () {\n    setIsMounted(true);\n  }, []);\n\n  var switchTheme = function switchTheme() {\n    if (isMounted) {\n      setTheme(theme === \"light\" ? \"dark\" : \"light\");\n    }\n  };\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: \"text-center\",\n    children: [/*#__PURE__*/_jsxs(Head, {\n      children: [/*#__PURE__*/_jsx(\"title\", {\n        children: \"Dark mode with Tailwind and Next.js\"\n      }), /*#__PURE__*/_jsx(\"link\", {\n        rel: \"icon\",\n        href: \"/favicon.ico\"\n      })]\n    }), /*#__PURE__*/_jsx(\"h1\", {\n      className: \"text:2xl\",\n      children: \"Dark mode with Tailwind and Next- themes\"\n    }), /*#__PURE__*/_jsx(\"button\", {\n      onClick: switchTheme,\n      children: \"Change theme\"\n    })]\n  });\n}","map":null,"metadata":{},"sourceType":"module"}