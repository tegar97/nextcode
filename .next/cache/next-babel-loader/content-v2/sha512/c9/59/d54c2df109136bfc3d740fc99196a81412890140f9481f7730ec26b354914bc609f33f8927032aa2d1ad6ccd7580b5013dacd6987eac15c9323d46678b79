{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useEffect, useState } from \"react\";\nimport { useTheme } from \"next-themes\";\nexport default function Home() {\n  var _useState = useState(false),\n      isMounted = _useState[0],\n      setIsMounted = _useState[1];\n\n  var _useTheme = useTheme(),\n      theme = _useTheme.theme,\n      setTheme = _useTheme.setTheme;\n\n  useEffect(function () {\n    setIsMounted(true);\n  }, []);\n\n  var switchTheme = function switchTheme() {\n    if (isMounted) {\n      setTheme(theme === \"light\" ? \"dark\" : \"light\");\n    }\n  };\n\n  return /*#__PURE__*/_jsx(\"button\", {\n    onClick: switchTheme,\n    children: /*#__PURE__*/_jsx(\"i\", {\n      className: \"\".concat(theme === 'light' ? 'fas fa-sun' : 'fas fa-moon', \" color-black-50 dark:color-white\"),\n      style: {\n        color: theme === 'light' ? '#000' : '#fff'\n      }\n    })\n  });\n}","map":null,"metadata":{},"sourceType":"module"}